<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.kaoping.entities">
	<class name="Teacher" table="kp_teacher" lazy="false">
		<!--主键策略 -->
		<id name="id" column="id">
			<generator class="native"></generator>
		</id>
		<property name="teacherId" column="teacher_id" />
		<property name="password" column="password" />
		<property name="name" column="name" />
		<property name="nation" column="nation" />
		<property name="sex" column="sex" />
		<property name="cardId" column="card_id" />
		<property name="academy" column="academy" />
		<property name="level" column="level" />
		<property name="status" column="status" />
		<property name="degree" column="degree" />
		<property name="dignity" column="dignity" />
		<property name="positional" column="positional" />
		<property name="positionalLevel" column="positional_level" />
		<property name="positionalSery" column="positional_sery" />
		<property name="loginNum" column="login_num" />
		<property name="type" column="type" />
	</class>
</hibernate-mapping>
